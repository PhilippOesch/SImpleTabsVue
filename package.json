{
    "name": "SimpleTabs-Vue",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/simple-tabs-vue.umd.cjs",
    "module": "./dist/simple-tabs-vue.js",
    "exports": {
        ".": {
            "import": "./dist/simple-tabs-vue.js",
            "require": "./dist/simple-tabs-vue.umd.cjs"
        },
        "./style.css": "./dist/style.css"
    },
    "scripts": {
        "generate:types": "vue-tsc --declaration --emitDeclarationOnly --outdir ./dist",
        "dev": "vite",
        "build": "vite build && npm run generate:types",
        "preview": "vite preview"
    },
    "dependencies": {
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@types/node": "^20.1.4",
        "@vitejs/plugin-vue": "^4.1.0",
        "typescript": "^5.0.2",
        "vite": "^4.3.2",
        "vitest": "^0.31.0",
        "vue-tsc": "^1.4.2"
    }
}
